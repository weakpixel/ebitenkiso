
//go:build ignore
//kage:unit pixels

package main

var (
    // Value offset value for red and green.
    Value vec2 
)
func Fragment(position vec4, texCoord vec2, _ vec4) vec4 {
    c := imageSrc0UnsafeAt(texCoord)
    c.r = imageSrc0At(texCoord - Value.x).r
    c.b = imageSrc0At(texCoord + Value.y).b
    // c.g = // untouched
	return c
}


