package main
//go:build ignore
//kage:unit pixels

func Fragment(position vec4, texCoord vec2, color vec4) vec4 {
    c := imageSrc0UnsafeAt(texCoord)
    if c.a > 0.0 {
        if c.r > 0.0 || c.g > 0.0 || c.b > 0.0 {
            return color
        }
    }
	return vec4(0.0, 0.0, 0.0, 0.0)
}
